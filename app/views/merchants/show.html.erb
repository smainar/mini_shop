<section id="merchant-id-<%= @merchant.id %>">
  <h1>Name: <%= @merchant.name %></h1>
  <h3>Address: <%= @merchant.address %></h3>
  <h3>City: <%= @merchant.city %></h3>
  <h3>State: <%= @merchant.state %></h3>
  <h3>Zip: <%= @merchant.zip %></h3>
</section>

<p><%= link_to "Edit Info", "/merchants/#{@merchant.id}/edit" %></p>
<p><%= link_to "Delete", "/merchants/#{@merchant.id}", confirm: "Are you sure?", method: :delete %></p>
